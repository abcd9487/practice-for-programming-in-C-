#include <stdio.h>
int ans, n, k, a[20];
void eq(int i,int sum)
{	
	if(sum == k){
     ans++; 
     return;
  }
	if(i == n) return;
	eq(i+1,sum+a[i]);
	eq(i+1,sum);
}
int main()
{
	int i;
	scanf("%d%d",&n,&k);
	for(i=0;i<n;i++)
		scanf("%d",&a[i]);
	eq(0,0);
	printf("%d\n",ans);
	return 0;
}
//若測資為 1 1 1 1 1，則答案是錯的
